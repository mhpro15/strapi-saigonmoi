      "target": "api::author.author"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category"
    },
    "Block": {
      "type": "dynamiczone",
      "components": [
        "shared.slider",
        "shared.seo",
        "shared.rich-text",
        "shared.quote",
        "shared.media",
        "shared.ck-editor"
      ]
